    <!-- MODAL -->
    <div tabindex="-1" class="modal fade" id="modalAterarSenha" aria-hidden="true" style="display: none;position: fixed">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Alterar Senha</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <div class="row">
                            <div class="form-group col-md-12">
                                <div class="form-group form-md-line-input has-info">
                                    <input type="password" id="txt_senha" class="form-control form-control-solid placeholder-no-fix form-group text-box single-line password" autocomplete="off" placeholder="Nova senha" required />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-12">
                                <div class="form-group form-md-line-input has-info">
                                    <input type="password" id="txt_confirmacao_senha" class="form-control form-control-solid placeholder-no-fix form-group text-box single-line password" autocomplete="off" placeholder="Confirme a senha" required />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div id="senhaBarra" class="progress" style="display: none;">
                        <div id="senhaForca" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 100%;"></div>
                    </div>
                    <div caria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="text-align:left; margin-top:5px; margin-bottom:18px">
                        <span id="senhaMsg" />
                    </div>

                    <div id="politica" caria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 100%;text-align:left;">
                        A senha dever ter, no mínimo, <strong>6</strong> caracteres e máximo <strong>16</strong> caracteres, dentre os quais deve haver, obrigatoriamente:<BR>
                        - Um número.<BR>
                        - Uma letra minúscula.<BR>
                        - Uma letra maiúscula.<BR>
                        - Um caracter especial.<BR>
                    </div>
                    <button class="btn blue" type="button" id="btnCancelarAlterarSenha">Cancelar</button>
                    <button class="btn blue" type="button" id="btnAlterarSenha">Salvar</button>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>


    @*Spinner*@
